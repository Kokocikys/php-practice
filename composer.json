{
	"name" : "kokocikys/php-practice.my",
	"description" : "Composer practice",
	"minimum-stability" : "stable",
	"license" : "proprietary",
	"authors" : [
		{
			"name" : "Илья Чернецкий",
			"email" : "chernecky_ilia@rambler.ru"
		}
	],
	"config" : {
		"sort-packages" : true
	},
	"require" : {
		"php" : "^8.0",
		"monolog/monolog" : "dev-main",
		"psr/http-message" : "dev-master",
		"psr/http-server-middleware" : "dev-master",
		"laminas/laminas-httphandlerrunner" : "1.4.0",
		"laminas/laminas-diactoros" : "2.6.0",
		"aura/router" : "^3.1.0"
	},
	"autoload" : {
		"psr-4" : {
			"App\\" : "src/App/",
			"Framework\\" : "src/Framework/",
			"Tests\\" : "tests/"
		}
	},
	"require-dev" : {
		"roave/security-advisories" : "dev-latest",
		"phpunit/phpunit" : "9.5.6"
	},
	"scripts" : {
		"serve" : "php -S 0.0.0.0:8080"
	}
}